console.log("Destroy!!!")

<!-- $=> search, # => select id -->
var curLikes = $("#<%= dom_id(@like.photo) %>_likes");

<!-- j => js, render the page that does the test. Then, grab the current object that's being changed. The render will replace it. -->
var destroyLike = $("<%= j(render "photos/likes", photo: @like.photo)%>")

curLikes.replaceWith(destroyLike)
